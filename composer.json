{
    "name": "letsgoi/nova-attach-many",
    "type": "library",
    "description": "Attach Many Nova field",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0|^5.0",
        "laravel/framework": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "NovaAttachMany\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NovaAttachMany\\Providers\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
